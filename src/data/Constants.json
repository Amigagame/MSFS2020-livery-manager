    "apiEndpoint": "https://api.liveriesmegapack.com",
    "devApiEndpoint": "http://localhost:2678",
    "cdnEndpoint": "https://manager-cdn.liveriesmegapack.com"
  },
  "dirs": {
    "downloadCache": "cache\\downloads"
  },
  "modLockFileName": ".livery-manager.json",
